{
  "type": "module",
  "name": "openapi-code",
  "version": "0.4.0",
  "description": "Toolchain for OpenAPI document as code",
  "keywords": [
    "openapi"
  ],
  "license": "MIT",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "default": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./openapi/v3.1": {
      "default": "./lib/openapi/v3.1/index.js",
      "types": "./lib/openapi/v3.1/index.d.ts"
    },
    "./eslint": {
      "default": "./lib/eslint/index.js",
      "types": "./lib/eslint/index.d.ts"
    }
  },
  "bin": {
    "openapi-code": "./lib/cli.js"
  },
  "repository": "github:KoharaKazuya/openapi-code-toolchain",
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "chalk": "^5.3.0",
    "escodegen": "^2.1.0",
    "esquery": "^1.5.0",
    "globby": "^14.0.0",
    "hono": "^4.6.10",
    "js-yaml": "^4.1.0",
    "openapi-format": "^1.15.2",
    "rollup": "^4.5.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "swagger-ui-dist": "^5.10.3",
    "zx": "^8.2.2"
  },
  "devDependencies": {
    "@types/escodegen": "^0.0.10",
    "@types/eslint": "^9.6.1",
    "@types/esquery": "^1.5.3",
    "@types/estree": "^1.0.5",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.9.0",
    "@typescript-eslint/typescript-estree": "^8.14.0",
    "@typescript-eslint/utils": "^8.14.0",
    "eslint": "^9.15.0",
    "typescript": "^5.3.2",
    "typia": "^7.5.1"
  },
  "peerDependencies": {
    "@ryoppippi/unplugin-typia": "^1",
    "typia": "^7"
  },
  "peerDependenciesMeta": {
    "@ryoppippi/unplugin-typia": {
      "optional": true
    },
    "typia": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  }
}
